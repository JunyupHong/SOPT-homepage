{
    "env": {
        "browser": true,
        "es6": true
    },
    // "extends": ["airbnb-typescript", "airbnb/hooks", "plugin:@typescript-eslint/recommended", "plugin:@typescript-eslint/recommended-requiring-type-checking"],
    "extends": ["airbnb", "plugin:@typescript-eslint/recommended", "prettier", "prettier/react"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": ["react", "@typescript-eslint", "prettier"],
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
        }
      }
}